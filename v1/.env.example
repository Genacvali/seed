# RabbitMQ configuration
RABBIT_HOST=127.0.0.1
RABBIT_USER=seed
RABBIT_PASS=seedpass
RABBIT_QUEUE=seed-inbox
RABBIT_VHOST=/

# MongoDB configuration
MONGO_URI=mongodb://username:password@host:27017/?authSource=admin&connect=direct
MONGO_STAGE_URI=mongodb://username:password@stage-host:27017/?authSource=admin&connect=direct

# Mattermost webhook
MM_WEBHOOK=https://your-mattermost.com/hooks/webhook-id
MM_VERIFY_SSL=1

# Telegraf configuration
TELEGRAF_URL=http://host:9216/metrics
TELEGRAF_PORT=9216
TELEGRAF_VERIFY_SSL=0

# LLM configuration
USE_LLM=0
GIGACHAT_CLIENT_ID=
GIGACHAT_CLIENT_SECRET=
GIGACHAT_OAUTH_URL=https://ngw.devices.sberbank.ru:9443/api/v2/oauth
GIGACHAT_API_URL=https://gigachat.devices.sberbank.ru/api/v1/chat/completions
GIGACHAT_SCOPE=GIGACHAT_API_PERS
GIGACHAT_MODEL=GigaChat-2
GIGACHAT_VERIFY_SSL=1
GIGACHAT_TOKEN_CACHE=

# Configuration
SEED_CONFIG=configs/seed.yaml